        android:layout_width="218dp"
        android:layout_height="49dp"
        android:layout_marginLeft="144dp"
        android:layout_marginTop="762dp"
        android:fontFamily="Montserrat"
        android:text="@string/braguia_string"
        android:textColor="@color/braguia_color"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/guideline9"
        app:layout_constraintEnd_toEndOf="@id/guideline12"
        app:layout_constraintStart_toStartOf="@id/guideline12"
        app:layout_constraintTop_toTopOf="@id/guideline9" />

    <ImageView android:id="@+id/location__1__2"
        android:src="@drawable/location__1__2"
        android:contentDescription="location__1__2"
        android:layout_marginStart="154dp"
        android:layout_marginTop="697dp"
        android:layout_width="49dp"
        android:layout_height="62dp"
        app:layout_constraintTop_toTopOf="@id/guideline10"
        app:layout_constraintBottom_toBottomOf="@id/guideline10"
        app:layout_constraintStart_toStartOf="@id/guideline"
        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintDimensionRatio="1:1"
        android:scaleType="centerCrop"/>

    <ImageButton
        android:id="@+id/btn_emmergency"
        android:background="@drawable/emmergency"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="@id/guideline11"
        app:layout_constraintBottom_toBottomOf="@id/guideline11"
        app:layout_constraintEnd_toEndOf="@id/guideline5"
        app:layout_constraintStart_toStartOf="@id/guideline5" />

</androidx.constraintlayout.widget.ConstraintLayout>