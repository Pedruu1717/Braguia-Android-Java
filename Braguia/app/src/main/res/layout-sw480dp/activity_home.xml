            android:fontFamily="Montserrat"
            android:layout_gravity="center_horizontal|center"
            android:gravity="center"
            android:textSize="20.7sp"
            android:textColor="@color/place_name_color"
            android:layout_marginLeft="4.96dp"
            android:layout_marginTop="89.99dp"
            android:layout_width="136.37832763672dp"
            android:layout_height="62.368327636719dp"
