                android:layout_width="15.28dp"
                android:layout_height="42.24dp"
                app:layout_constraintDimensionRatio="1:1"
                android:scaleType="centerCrop"/>

        </RelativeLayout>

        <TextView android:id="@+id/place_name"
            android:fontFamily="Montserrat"
            android:layout_gravity="center_horizontal|center"
            android:gravity="center"
            android:textSize="20.7sp"
            android:textColor="@color/place_name_color"
            android:layout_marginLeft="3.96dp"
            android:layout_marginTop="81.95dp"
            android:layout_width="136.37832763672dp"
            android:layout_height="62.368327636719dp"
            android:text="@string/place_name_string"
            />

    </RelativeLayout>


    <RelativeLayout android:id="@+id/place_card_ek1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/guideline6"
        app:layout_constraintBottom_toBottomOf="@id/guideline6"
        app:layout_constraintStart_toStartOf="@id/guideline8"
        app:layout_constraintEnd_toEndOf="@id/guideline8">

        <View
