                android:layout_marginStart="6.9dp"
                android:layout_marginTop="2.08dp"
                android:layout_width="36.2dp"
                android:layout_height="45.83dp" />

            <ImageView
                android:id="@+id/vector_ek4"
                android:layout_width="13.79dp"
                android:layout_height="13.79dp"
                android:layout_marginStart="18.12dp"
                android:layout_marginTop="13.69dp"
                android:contentDescription="vector_ek4"
                android:src="@drawable/vector_ek4"
                app:layout_constraintDimensionRatio="1:1"
                android:scaleType="centerCrop"/>

        </RelativeLayout>

        <TextView android:id="@+id/place_name_ek1"
            android:fontFamily="Montserrat"
            android:layout_gravity="center_horizontal|center"
            android:gravity="center"
            android:textSize="20.7sp"
            android:textColor="@color/place_name_color"
            android:layout_marginLeft="4.96dp"
            android:layout_marginTop="82.99dp"
            android:layout_width="136.37832763672dp"
            android:layout_height="62.368327636719dp"
            android:text="@string/place_name_ek1_string"
            />

    </RelativeLayout>


    <RelativeLayout android:id="@+id/place_card_ek2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/guideline7"
        app:layout_constraintBottom_toBottomOf="@id/guideline7"
        app:layout_constraintStart_toStartOf="@id/guideline5"
        app:layout_constraintEnd_toEndOf="@id/guideline5"
        >

        <View
