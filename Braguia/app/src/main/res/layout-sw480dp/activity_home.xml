            android:layout_width="143.51dp"
            android:layout_height="152.51dp"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/container_ek2_shape" />

        <TextView android:id="@+id/place_name_ek2"
            android:fontFamily="Montserrat"
            android:layout_gravity="center_horizontal|center"
            android:gravity="center"
            android:textSize="20.7sp"
            android:textColor="@color/place_name_color"
            android:layout_marginLeft="4.96dp"
            android:layout_marginTop="89.99dp"
            android:layout_width="136.37832763672dp"
            android:layout_height="62.368327636719dp"
            android:text="@string/place_name_ek2_string"
            />


        <RelativeLayout android:id="@+id/icon_image"
            android:layout_marginTop="33dp"
            android:layout_marginStart="45dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >


            <RelativeLayout android:id="@+id/icon_image_ek1"
                android:layout_marginTop="0dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <ImageView
                    android:id="@+id/clip_path_group"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:contentDescription="clip_path_group"
                    android:src="@drawable/clip_path_group"
                    app:layout_constraintDimensionRatio="1:1"
                    android:scaleType="centerCrop"/>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>


    <RelativeLayout android:id="@+id/place_card_ek3"
